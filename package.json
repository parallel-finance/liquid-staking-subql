{
  "name": "my-staking-subql",
  "version": "0.0.4",
  "description": "This SubQuery project can be use as a starting point for parallel-heiko network",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -b",
    "prepack": "rm -rf dist && npm build",
    "ipfs:heiko": "subql publish -f ./deploy-heiko.yaml",
    "ipfs:para": "subql publish -f ./deploy-parallel.yaml",
    "test": "jest",
    "codegen": "./node_modules/.bin/subql codegen"
  },
  "homepage": "https://github.com/subquery/parallel-heiko-subql-starter",
  "repository": "github:subquery/parallel-heiko-subql-starter",
  "files": [
    "dist",
    "schema.graphql",
    "project.yaml"
  ],
  "author": "Parallel Finance",
  "license": "MIT",
  "devDependencies": {
    "@parallel-finance/type-definitions": "latest",
    "@polkadot/api": "^7",
    "@subql/cli": "latest",
    "@subql/types": "latest",
    "typescript": "^4.1.3"
  },
  "exports": {
    "chaintypes": "./src/chaintypes.ts"
  }
}
